{
  "name": "xc-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "element-ui": "^2.13.0",
    "js-base64": "^2.5.2",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "wangeditor": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "semi": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "always"
      ],
      "brace-style": [
        1,
        "1tbs"
      ],
      "no-dupe-keys": 1,
      "no-alert": 1,
      "no-loop-func": 1,
      "no-multi-spaces": 1,
      "no-redeclare": 1,
      "no-undef-init": 1,
      "no-unused-vars": 1,
      "camelcase": 1,
      "func-call-spacing": 1,
      "indent": [
        1,
        "tab"
      ],
      "key-spacing": [
        1,
        {
          "beforeColon": false
        }
      ],
      "lines-around-comment": [
        1,
        {
          "beforeBlockComment": true
        }
      ],
      "no-mixed-spaces-and-tabs": 1,
      "no-multiple-empty-lines": 1,
      "no-trailing-spaces": [
        1,
        {
          "ignoreComments": true
        }
      ],
      "quotes": [
        1,
        "single"
      ],
      "space-infix-ops": 1,
      "spaced-comment": [
        1,
        "always"
      ],
      "no-const-assign": 1,
      "no-var": 1,
      "prefer-template": 1,
      "space-before-blocks": [
        1,
        "always"
      ],
      "no-debugger": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
